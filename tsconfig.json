{
  "compilerOptions": {
    "target": "es2016",
    "useDefineForClassFields": true,
    "preserveSymlinks": true,
    "baseUrl": ".",
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUncheckedIndexedAccess": true,
    "module": "commonjs",
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  }
}
